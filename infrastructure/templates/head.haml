- load staticfiles
- load django_select2_tags
- load static from staticfiles
- load compress
%html
  %head
    %title
      - block title
    %script{src: '{{ STATIC_URL }}js/jquery.js' }
    %script{src: '{{ STATIC_URL }}js/storyjs-embed.js' }
    %script{src: '{{ STATIC_URL }}js/main.js' }
    %script{src: 'http://api.tiles.mapbox.com/mapbox.js/v1.3.1/mapbox.js' }
    %script{src: '{{ STATIC_URL }}js/esri-leaflet.js' }
    %script{src: '{{ STATIC_URL }}js/proj4-compressed.js' }
    %script{src: '{{ STATIC_URL }}js/proj4leaflet.js' }
    %script{ src:"http://d3js.org/d3.v3.min.js", charset:"utf-8" }
    - compress css
      %link{href: "{% static 'css/normalize.css' %}", rel:'stylesheet', type:'text/css'}
      %link{href: "{% static 'css/application.scss' %}", rel:'stylesheet', type:'text/x-scss'}
      %link{href: "{% static 'css/timeline.css' %}", rel:'stylesheet', type:'text/css'}
      - import_django_select2_css
	  - import_django_select2_js
    %link{ href:'http://api.tiles.mapbox.com/mapbox.js/v1.3.1/mapbox.css', rel:'stylesheet' }
    %meta{ name:"viewport", content:"width=device-width, initial-scale=1.0" }
  %body
    - include 'navigation.html'

